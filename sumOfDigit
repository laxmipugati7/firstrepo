import java.util.Scanner;
public class sumOfDigit {
    public  static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter digits");
        int digit = sc.nextInt();
        int sum=0;

        while(digit>0){
            sum=sum+digit%10;
            digit=digit/10;
        }
        System.out.println(sum);
    }
}
