import java.util.Scanner;

public class primeNum {
    public static void main(String[] args) {
        int number=9;
        boolean prime=true;

        Scanner sc = new Scanner(System.in);
        System.out.println("enter a number");
        number = sc.nextInt();

        if(number<=1){
            prime=false;
        }
        else{
            for(int i=2;i<=9/2;i++){
                if(number%i==0){
                    prime=false;
                }
            }
        }
        System.out.println(prime?"Prime":"Not prime");


    }
}
