public class sumTrueFalse {
    public static void main(String[] args) {
        int num1=8;
        int num2=2;
        int sum;

        if(num1==6||num2==6||num1+num2==6||num1-num2==6){
            System.out.println ("true");
        }
        else{
            System.out.println ("false");

        }

    }
}
